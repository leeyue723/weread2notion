/* 无边框，结合底色使用 */
.noborder {
    border: none;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 单线边框 */
.solid {
    border: solid 1px;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 虚线边框 */
.dashed {
    border: dashed 3px #39c;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 点边框 */
.dotted {
    border: dotted 3px #dedede;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 上下双线 */
.dualline {
    border-top: double 3px #000;
    border-bottom: double 3px #000;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 左右单线 */
.doubleline {
    border-left: solid 2px #C00;
    border-right: solid 2px #C00;
    margin: 1em 0.5em;
    padding: 0.8em 0.5em
    }
/* 棕色圆角左右框*/
.lrkuang {
    border-left: 2px dashed #A0522D;
    border-right: 2px dashed #A0522D;
    padding: 0.8em 0.5em;
    border-radius: 0.6em;
    margin: 1em 0.5em
    }
/* 左侧粗竖线 */
.single {
    border-left: #bebebe 0.35em solid;
    padding-left: 0.5em
    }
blockquote.single {
    margin-top: 1em;
    margin-bottom: 0;
    margin-left: 0.5em;
    margin-right: 0.5em
    }
/* 边框带阴影 */
.shadow {
    padding: 0.8em;
    border: 1px solid silver;
    margin: 1em 0.5em 1em 0.5em;
    -moz-box-shadow: 2px 2px 2px silver;
    -webkit-box-shadow: 2px 2px 2px silver;
    box-shadow: 2px 2px 2px silver
    }
p .shadow {
    margin-top: 0;
    margin-bottom: 0
    }
div .shadow {
    margin-top: 0;
    margin-bottom: 0
    }
/* 圆角 */
.round {
    margin: 1em 0.5em;
    padding: 0.8em 0.5em;
    -moz-border-radius: 19px;
    -webkit-border-radius: 19px;
    border-radius: 19px
    }
/* 向右倾斜 */
.lean {
    -webkit-transform: rotate(4deg) scale(0.861) skew(1deg) translate(0);
    -moz-transform: rotate(4deg) scale(0.861) skew(1deg) translate(0);
    -o-transform: rotate(4deg) scale(0.861) skew(1deg) translate(0)
    }
