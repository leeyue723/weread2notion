body {
    padding: 0 0;
    margin: 0 0
    }
/* 如果使用全屏需要留出三字符的边框 */
body.banxin {
    padding: 10%
    }
/*=========================段落格式=========================*/
/* 标题一 篇*/
h1 {
    font-size: 1.7em;
    margin-top: 2em;
    margin-bottom: 1em;
    text-align: center;
    font-weight: normal;
    text-indent: 0
    }
/* 标题二 章*/
h2 {
    font-size: 1.6em;
    margin-top: 2em;
    margin-bottom: 1.3em;
    text-align: center;
    color: #03C;
    text-indent: 0;
    font-weight: 800
    }
/* 标题三 节*/
h3 {
    font-size: 1.5em;
    margin-top: 2em;
    margin-bottom: 1.3em;
    text-align: left;
    text-indent: 0;
    font-weight: normal
    }
/* 标题四 小标题*/
h4 {
    font-size: 1.3em;
    margin-top: 1em;
    margin-bottom: 0.5em;
    text-align: left;
    text-indent: 2em;
    font-weight: 800
    }
/* 标题五 图表标题*/
h5 {
    font-size: 1em;
    font-weight: bold;
    text-align: center;
    text-indent: 2em;
    margin: 0
    }
/* 标题 与图间隔*/
.chatu + h5 {
    margin-top: 0.5em
    }
h5 + table {
    margin-top: 0.5em
    }
h5 + table.narrow {
    margin-top: 0.5em
    }
table + h5 {
    margin-top: 1.5em
    }
.tushuo + h5 {
    margin-top: 0.5em
    }
/* 标题 与正文间隔*/
h5 + p {
    margin-top: 1.5em
    }
p + h5 {
    margin-top: 1.5em
    }
h5 + blockquote {
    margin-top: 1.5em
    }
blockquote + h5 {
    margin-top: 1.5em
    }
/* 标题六 */
h6 {
    font-size: 1em;
    margin: 0.5em 0 0 0;
    text-align: left;
    text-indent: 0;
    line-height: 1.2em;
    font-weight: normal
    }
h6 + table {
    margin-top: 0
    }
table + h6 {
    margin-top: 0
    }
h5 + h6 {
    margin-top: 0.5em
    }
h6 + p {
    margin-top: 1.5em
    }
p + h6 {
    margin-top: 1.5em
    }
h6 + h6 {
    margin-top: 0
    }
/* 图表说-楷体 */
.tushuo {
    font-size: 1em;
    margin: 0.5em 0 0 0;
    text-align: left;
    font-family: kaiti !important;
    text-indent: 0;
    line-height: 1.2em
    }
.tushuo + table {
    margin-top: 0
    }
table + .tushuo {
    margin-top: 0
    }
h5 + .tushuo {
    margin-top: 0.5em
    }
.tushuo + p {
    margin-top: 1.5em
    }
p + .tushuo {
    margin-top: 1.5em
    }
table + p {
    margin-top: 1.5em
    }
.tushuo + .tushuo {
    margin-top: 0
    }
/* 正文 */
p {
    font-size: 1.15em;
    text-align: justify;
    text-indent: 2em;
    margin-top: 1em;
    margin-bottom: 0;
    text-justify: inter-ideograph;
    line-height: 1.5em;
    letter-spacing: 0
    }
/* 引文 */
blockquote {
    margin-top: 1em;
    margin-bottom: 0;
    margin-left: 1em;
    margin-right: 1em
    }
blockquote p {
    font-family: kaiti !important;
    font-size: 1.15em;
    margin-top: 0.5em;
    margin-bottom: 0;
    text-indent: 2em;
    text-align: justify;
    font-weight: normal;
    line-height: 1.4em;
    text-justify: inter-ideograph
    }
/* 引文 与正文间隔*/
p + blockquote {
    margin-top: 2em
    }
blockquote + p {
    margin-top: 2em
    }
div blockquote {
    margin: 0.5em 0
    }
div p + blockquote {
    margin-top: 1.5em
    }
div blockquote + p {
    margin-top: 1.5em
    }
div p {
    margin: 0.5em 0
    }
blockquote + blockquote {
    margin-top: 0
    }
/* 段中引文 */
/* 表格 黑色边框 */
table {
    border-collapse: collapse;
    border: 2px solid black;
    margin: 1.5em auto 0 auto;
    width: 100%
    }
td, th {
    border: 1px solid black;
    padding: 0.4em;
    font-family: heiti !important;
    font-size: 1em;
    word-break: break-all;
    text-indent: 0 !important
    }
td p {
    margin: 0;
    font-family: heiti !important;
    font-size: 1em;
    text-indent: 0 !important;
    text-align: left;
    text-justify: auto;
    word-break: break-all
    }
td p b {
    font-size: 1em
    }
table + p {
    margin-top: 1.5em
    }
/* 无竖线表格 */
table.halfline {
    border-left: 0;
    border-right: 0
    }
table.halfline td {
    border-left: 0;
    border-right: 0
    }
table.halfline th {
    border-left: 0;
    border-right: 0
    }
/* 仅边框表格 */
table.borderline td {
    border-left: 0;
    border-right: 0;
    border-top: 0;
    border-bottom: 0
    }
table.borderline td {
    border-left: 0;
    border-right: 0;
    border-top: 0
    }
/* 无线表格 */
table.noline {
    border: 0
    }
table.noline td {
    border: 0
    }
table.noline th {
    border: 0
    }
/* 平均分布各列 */
.average {
    table-layout: fixed
    }
/* 表格不撑满宽度 */
.narrow {
    margin: 1.5em 0 0 10%;
    width: auto
    }
/* 脚注 */
.fnote {
    font-size: 0.9em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0
    }
p + .fnote {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
div + .fnote {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
h6 + .fnote {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
blockquote + .fnote {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
.fnote p {
    font-size: 1em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0
    }
.fnote + .fnote {
    margin-top: 0.5em;
    padding-top: 0;
    border-top: 0
    }
/* 无需链接的脚注 */
.footnote {
    font-size: 0.9em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0
    }
p + .footnote {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
div + .footnote {
    margin-top: 1em
    }
h6 + .footnote {
    margin-top: 1em
    }
blockquote + .footnote {
    margin-top: 1em
    }
.footnote p {
    font-size: 1em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0
    }
.footnote + .footnote {
    margin-top: 0.5em
    }
.fnote + .footnote {
    margin-top: 0.5em
    }
ol.footnote-content {
    font-size: 0.9em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0;
    font-family: none
    }
p + .footnote-content {
    margin-top: 1em;
    padding-top: 0.5em;
    border-top: solid 1px gray
    }
div + .footnote-content {
    margin-top: 1em
    }
h6 + .footnote-content {
    margin-top: 1em
    }
blockquote + .footnote-content {
    margin-top: 1em
    }
.footnote-content p {
    font-size: 1em;
    text-align: justify;
    text-indent: 2em;
    line-height: 1.5em;
    margin-top: 0;
    margin-bottom: 0
    }
.footnote-item + .footnote-item {
    margin-top: 0.5em
    }
/* 避免标题重影 */
h1, h3 {
    font-weight: normal
    }
/* 列表项 */
ul, ol {
    margin-top: 1em;
    margin-bottom: 0;
    font-size: 1.15em;
    font-family: songti;
    text-align: justify;
